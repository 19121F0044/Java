import java.util.Scanner;

class Solution
{
	
	public static void main(String[] args)
	{
		Scanner scn=new Scanner(System.in);
		int n1=scn.nextInt();
		int fact =1;
		for(int i=1;i<=n1;i++)
		{
			fact=fact*i;
		}
		int count=0;
		while(fact!=0)
		{
			int rem=fact%10;
			if(rem==0)
			{
				count++;
				
			
				
			}
			else
			{

					break;
			}
			fact=fact/10;
		}
		System.out.print(count);

		
	}

}

