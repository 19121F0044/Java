import java.util.Scanner;
class Solution
{
	public static void main(String[] args)
	{
		Scanner scn=new Scanner(System.in);
		int n=scn.nextInt();
		if(n%4==0)
		{
			System.out.println("It is leaf year");
		}
		else if(n%400==0 && n%100!=0)
		{
			System.out.println("It is leaf year");
		}
		else
		{
			System.out.println("It is not a leap year");
		}

	}

}
