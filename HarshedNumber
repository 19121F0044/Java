import java.util.Scanner;
class Solution
{
	public static void main(String[] args)
	{
		Scanner scn=new Scanner(System.in);
		int n=scn.nextInt();
		int temp=n;
		int sum=0;
		while(n>0)
		{
			sum=sum+(n%10);
			n/=10;
		}
		if(temp%sum==0)
		{
			System.out.print("It is harshed number");
		}
		else
		{
			System.out.print("It is not harshed number");
		}

	}

}
